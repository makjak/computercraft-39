
turtle.dig()
t.forward()
turtle.dig()

local height = 0

turtle.select(2)
while turtle.compareUp() do
	turtle.digUp()
	t.up()
	turtle.dig()
	height = height + 1
	turtle.select(2)
end

t.turnLeft()
turtle.dig()
t.forward()
t.turnRight()
turtle.dig()

while height > 0 do
	turtle.digDown()
	t.down()
	turtle.dig()
	height = height - 1
end

while t.dn() do end

