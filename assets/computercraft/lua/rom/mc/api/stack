function new()
	local stck = {}
	stck.head = nil

	-- functions
	stck.pop = pop
	stck.push = push
	stck.peek = peek

	return stck
end

function pop(self)
	local head = self.head

	if head == nil then
		return nil
	end

	local value = head.value

	self.head = head.next

	return value
end

function push(self, obj)
	local newHead = {}
	newHead.value = obj
	newHead.next = self.head

	self.head = newHead
end

function peek(self)
	print(self)
	local h = self.head

	if h == nil then
		return nil
	end

	return h.value
end